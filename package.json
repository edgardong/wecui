  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build:wecui": "webpack --hide-modules --config build/webpack.build.js && rimraf examples/dist && cross-env NODE_ENV=production webpack --hide-modules --config build/webpack.build.min.js",
    "build:wecui-css": "gulp build --gulpfile packages/wecui-css/gulpfile.js && rimraf lib/wecui-css && cp-cli packages/wecui-css/lib lib/wecui-css && rimraf packages/wecui-css/lib",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js"
  },
